

<nav class="navbar navbar-default">
   <ul class="nav navbar-nav">
      <li><a href="/projects/new/<%= current_user.id %>">New Project</a></li> 
      <% if current_user %>
      <li><a href="/users/sign_out" data-method="delete" rel="nofollow">LogOut</a></li> 
    <% end %>       
    </ul>
  </div>
</nav>
<div class="container">
<br>

<font color="red"><h3>Welcome <%= current_user.name %></h3></font><br />
<font color="blue"><h4>Available Projects</h4></font>
<div class="col-md-7">
<table class="table">
  <thead>
    <tr>
      <th><font color="#337ab7">Name</th></font>
      <th><font color="#337ab7">Code</th></font>
      <th><font color="#337ab7">Start Date</th></font>
      <th><font color="#337ab7">End Date</th></font>
       <th><font color="#337ab7">Status</th></font>

      <th></th>
    </tr>
  </thead>

  <tbody>
 <% @projects.each do |project| %>
      <tr>      
        <td><%= project.name %></td>
        <td><%= project.code %></td>
        <td><%= project.startdate %></td>
        <td><%= project.enddate %></td>
        <td><%= project.status %></td>
         <td><%= link_to "Tasks", "/projectdetails/#{project.id}", class: "btn btn-info btn-xs" %></td>
          <td><%= link_to "Edit", "/editproject/#{project.id}", class: "btn btn-info btn-xs" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

</div>